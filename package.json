{
  "name": "@gofynd/storefront-tag-templates",
  "version": "1.0.0",
  "description": "A collection of tag templates for Fynd Platform storefronts including Google Tag Manager, Sentry, and Google Analytics integrations",
  "main": "dist/templates.js",
  "module": "dist/templates.esm.js",
  "types": "dist/templates.d.ts",
  "exports": {
    ".": {
      "require": "./dist/templates.js",
      "import": "./dist/templates.esm.js",
      "default": "./dist/templates.js"
    },
    "./dist/template": "./dist/template.js"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gofynd/fynd-storefront-tag-template.git"
  },
  "keywords": [
    "fynd",
    "storefront",
    "tags",
    "google-tag-manager",
    "gtm",
    "sentry",
    "google-analytics",
    "analytics",
    "ecommerce",
    "tracking"
  ],
  "author": "Fynd Platform",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gofynd/fynd-storefront-tag-template/issues"
  },
  "homepage": "https://github.com/gofynd/fynd-storefront-tag-template#readme",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
} 